{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Prometheus data source",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
    
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.x" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "10.x" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "10.x" },
    { "type": "panel", "id": "bargauge", "name": "Bar gauge", "version": "10.x" },
    { "type": "panel", "id": "table", "name": "Table", "version": "10.x" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "2.x" }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "Requests",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "aurora_requests_total",
          "legendFormat": "total",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "colorMode": "value", "graphMode": "none" }
    },
    {
      "title": "Error rate",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        { "expr": "aurora_request_error_rate", "legendFormat": "rate", "refId": "A" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "colorMode": "value", "graphMode": "none" },
      "fieldConfig": { "defaults": { "unit": "percentunit", "min": 0, "max": 1 } }
    },
    {
      "title": "Latency p50/p95/p99 (ms)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        { "expr": "aurora_request_latency_p50_ms", "legendFormat": "p50", "refId": "A" },
        { "expr": "aurora_request_latency_p95_ms", "legendFormat": "p95", "refId": "B" },
        { "expr": "aurora_request_latency_p99_ms", "legendFormat": "p99", "refId": "C" }
      ]
    },
    {
      "title": "Webhooks registered",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 4 },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [ { "expr": "aurora_webhooks_registered", "refId": "A" } ]
    },
    {
      "title": "Webhook queue depth",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 4 },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [ { "expr": "aurora_webhook_queue_depth", "refId": "A" } ]
    },
    {
      "title": "Orders total",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 4 },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [ { "expr": "aurora_orders_total", "refId": "A" } ]
    },
    {
      "title": "Marketplace items total",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 8 },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [ { "expr": "aurora_marketplace_items_total", "refId": "A" } ]
    },
    {
      "title": "Hybrid cache hit ratio",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 8 },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [ { "expr": "aurora_hybrid_cache_hit_ratio", "refId": "A" } ],
      "fieldConfig": { "defaults": { "unit": "percentunit", "min": 0, "max": 1 } }
    },
    {
      "title": "Usage units by product (current period)",
      "type": "bargauge",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 8 },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [ { "expr": "aurora_usage_units_total", "legendFormat": "{{product}}", "refId": "A" } ]
    },
    {
      "title": "Eval summary",
      "type": "table",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 12 },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        { "expr": "aurora_evals_faithfulness", "refId": "A" },
        { "expr": "aurora_evals_relevancy", "refId": "B" },
        { "expr": "aurora_evals_recall", "refId": "C" }
      ],
      "options": { "showHeader": true }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["aurora", "phase4"],
  "templating": {
    "list": [
      {
        "name": "ds",
        "type": "datasource",
        "query": "prometheus",
        "label": "Data source",
        "current": { "text": "Prometheus", "value": "${DS_PROMETHEUS}" }
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": { "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"] },
  "timezone": "browser",
  "title": "Aurora â€” Phase 4 Ops",
  "version": 1
}
