{
  "__inputs": [
    { "name": "DS_PROMETHEUS", "label": "Prometheus", "description": "Prometheus data source", "type": "datasource", "pluginId": "prometheus", "pluginName": "Prometheus" }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.x" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "10.x" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "10.x" },
    { "type": "panel", "id": "bargauge", "name": "Bar gauge", "version": "10.x" },
    { "type": "panel", "id": "table", "name": "Table", "version": "10.x" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "2.x" }
  ],
  "annotations": { "list": [ { "builtIn": 1, "datasource": { "type": "grafana", "uid": "-- Grafana --" }, "enable": true, "hide": true, "iconColor": "rgba(0, 211, 255, 1)", "name": "Annotations & Alerts", "type": "dashboard" } ] },
  "editable": true,
  "graphTooltip": 0,
  "panels": [
    { "title": "KG nodes", "type": "stat", "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 }, "datasource": "${DS_PROMETHEUS}", "targets": [ { "expr": "aurora_kg_nodes_total", "refId": "A" } ] },
    { "title": "KG edges", "type": "stat", "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 }, "datasource": "${DS_PROMETHEUS}", "targets": [ { "expr": "aurora_kg_edges_total", "refId": "A" } ] },
    { "title": "Agents running", "type": "stat", "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 }, "datasource": "${DS_PROMETHEUS}", "targets": [ { "expr": "aurora_agents_running_total", "refId": "A" } ] },
    { "title": "Certified analysts", "type": "stat", "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 }, "datasource": "${DS_PROMETHEUS}", "targets": [ { "expr": "aurora_certified_analysts_total", "refId": "A" } ] },
    { "title": "Success-fee agreements", "type": "stat", "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 }, "datasource": "${DS_PROMETHEUS}", "targets": [ { "expr": "aurora_success_fee_agreements_total", "refId": "A" } ] },
    { "title": "Latency p50/p95/p99 (ms)", "type": "timeseries", "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 }, "datasource": "${DS_PROMETHEUS}", "targets": [ { "expr": "aurora_request_latency_p50_ms", "legendFormat": "p50", "refId": "A" }, { "expr": "aurora_request_latency_p95_ms", "legendFormat": "p95", "refId": "B" }, { "expr": "aurora_request_latency_p99_ms", "legendFormat": "p99", "refId": "C" } ] },
    { "title": "Usage units by product (current period)", "type": "bargauge", "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 }, "datasource": "${DS_PROMETHEUS}", "targets": [ { "expr": "aurora_usage_units_total", "legendFormat": "{{product}}", "refId": "A" } ] }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["aurora", "phase5"],
  "time": { "from": "now-6h", "to": "now" },
  "title": "Aurora â€” Phase 5 Ops",
  "version": 1
}