{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Repos",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "repo_url": {"type": "string"},
      "stars": {"type": "integer"},
      "forks": {"type": "integer"},
      "topics": {"type": "array", "items": {"type": "string"}},
      "last_commit_at": {"type": "string"},
  "company_id": {"type": ["integer", "string"]}
    },
    "required": ["repo_url"]
  }
}
